{
  "version": 2,
  "buildCommand": "bash build_files.sh",
  "outputDirectory": "staticfiles",
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "vercel_app.py"
    }
  ]
}