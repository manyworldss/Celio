{
  "version": 2,
  "buildCommand": "sh build_files.sh",
  "outputDirectory": "staticfiles",
  "functions": {
    "vercel_app.py": {
      "runtime": "python3.10"
    }
  },
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/vercel_app.py"
    }
  ]
}